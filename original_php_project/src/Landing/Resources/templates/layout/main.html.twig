{% if not isTimezoneDetected() %}
    <script>
        window.addEventListener('DOMContentLoaded', (event) => {

            const tz = Intl.DateTimeFormat().resolvedOptions().timeZone;

            const url = "{{ url(constant("\\App\\Landing\\Service\\RouteNames::SAVE_USER_TIMEZONE")) }}";
            const params = {
                tz
            };

            fetch(url, {
                method: 'POST',
                headers: {
                    'Accept': 'application/json, text/plain, */*',
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify(params)
            }).then(res => res.json())
                .then(json => {
                    if (typeof mustRestartPageAfterDetectingTz !== 'undefined' && mustRestartPageAfterDetectingTz) {
                        window.location.reload();
                    }
                });
        });
    </script>
{% endif %}

