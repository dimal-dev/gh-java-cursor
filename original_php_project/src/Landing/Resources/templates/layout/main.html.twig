<script>
    window.addEventListener('DOMContentLoaded', (event) => {
        document.querySelector('.gh-header__burger-icon-mobile').addEventListener('click', (e) => {
            document.querySelector('.gh-mobile-header').classList.add('gh-mobile-header--opened');
        });
        document.querySelector('.gh-mobile-header__close-icon').addEventListener('click', (e) => {
            document.querySelector('.gh-mobile-header').classList.remove('gh-mobile-header--opened');
        });
    });
</script>

