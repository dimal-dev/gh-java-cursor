<header class="gh-header {{ headerWithoutShadow is defined and headerWithoutShadow ? '' : 'gh-header--with-shadow' }}">
    <a href="{{ url(constant('\\App\\Landing\\Service\\RouteNames::MAIN_PAGE')) }}" class="gh-header__logo">
        <img class="gh-img" src="{{ asset('img/logo/logo-olive.svg', 'landing') }}" alt="">
    </a>
    <nav class="gh-header__nav">
        <ul class="gh-header__nav-menu">
            <li class="{{ app.request.get('_route') == constant('\\App\\Landing\\Service\\RouteNames::PSIHOLOG_LIST') ? 'active' : '' }}">
                <a href="{{ url(constant('\\App\\Landing\\Service\\RouteNames::PSIHOLOG_LIST')) }}">
                    {{ 'our_specialists'|trans }}
                </a>
            </li>
            <li class="{{ app.request.get('_route') == constant('\\App\\Landing\\Service\\RouteNames::BLOG_POST_LIST') ? 'active' : '' }}">
                <a href="{{ url(constant('\\App\\Landing\\Service\\RouteNames::BLOG_POST_LIST')) }}">
                    {{ 'Blog'|trans }}
                </a>
            </li>
            <li>
                <a href="https://forms.gle/kVM9qC4Q2tMaD9Xe7" target="_blank">
                    {{ 'landing.header.for_psiholog'|trans }}
                </a>
            </li>
            <li>
                {% if isUserAuthenticated() %}
                    <a href="{{ url(constant('\\App\\User\\Service\\RouteNames::DASHBOARD')) }}">
                        {{ 'landing.header.user_dashboard'|trans }}
                    </a>
                {% else %}
                    <a href="{{ url(constant('\\App\\User\\Service\\RouteNames::LOGIN')) }}">
                        {{ 'landing.header.user_login'|trans }}
                    </a>
                {% endif %}
            </li>
        </ul>
    </nav>
    <div class="gh-header__lang-and-select-block-container">
        {% block headerLangPicker %}
            <div class="gh-header__lang-picker">
                <div class="gh-header__lang-picker-item {{ app.request.get('_locale') == 'ua' ? 'gh-header__lang-picker-item--active' : '' }}">
                    <a href="{{ currentRouteLocalized({_locale: 'ua'}) }}">UA</a>
                </div>
                <div class="gh-header__lang-picker-item {{ app.request.get('_locale') == 'ru' ? 'gh-header__lang-picker-item--active' : '' }}">
                    <a href="{{ currentRouteLocalized({_locale: 'ru'}) }}">RU</a>
                </div>
            </div>
        {% endblock %}
        <div class="gh-header__select-psiholog-btn-container">
            <a href="{{ url(constant('\\App\\Landing\\Service\\RouteNames::REQUEST_PSIHOLOG')) }}" class="select-psiholog-btn select-psiholog-btn--no-shadow select-psiholog-btn--header gh-button">
                {{ 'landing.request_psiholog'|trans }}
            </a>
        </div>
    </div>
    <div class="gh-header__burger-icon-mobile">
        <svg width="21" height="18" viewBox="0 0 21 18" fill="none" xmlns="http://www.w3.org/2000/svg">
            <rect x="0.481934" width="20" height="2" rx="1" fill="#202020"/>
            <rect x="0.481934" y="8" width="20" height="2" rx="0.999999" fill="#202020"/>
            <rect x="5.48193" y="16" width="15" height="2" rx="1" fill="#202020"/>
        </svg>
    </div>
</header>

