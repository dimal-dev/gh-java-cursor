<div id="psiholog-profile-photo-carousel" class="carousel slide" data-bs-ride="false">
    <div class="carousel-inner">
        {% for index, photoSrc in photos %}
            <div class="carousel-item {{ index == 0 ? 'active' : '' }}" data-bs-interval="10000">
                <div class="w-100">
                    <picture>
                        <source type="image/webp" srcset="{{ photoSrc.webp }}" class="img-fluid rounded">
                        <img src="{{ photoSrc.fallback }}" class="img-fluid rounded">
                    </picture>
                </div>
            </div>
        {% endfor %}
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#psiholog-profile-photo-carousel"
            data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#psiholog-profile-photo-carousel"
            data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>