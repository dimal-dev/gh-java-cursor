    <script>
        window.addEventListener('DOMContentLoaded', (event) => {
            const swiper = new Swiper('.swiper', {
                // Optional parameters
                // direction: 'vertical',
                // loop: true,

                // If we need pagination
                pagination: {
                    el: ".ghl-reviews__pagination",
                    clickable: true,
                },

                // Navigation arrows
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },

                // And if we need scrollbar
                scrollbar: {
                    el: '.swiper-scrollbar',
                },
                slidesPerView: 1,
                spaceBetween: 30,
                breakpoints: {
                    768: {
                        slidesPerView: 2,
                        spaceBetween: 30,
                    },
                    1200: {
                        slidesPerView: 3,
                        spaceBetween: 40,
                    },
                }
            });

            document.querySelectorAll('.select-psiholog-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    gtag("event", "select_psiholog_btn_clicked_from_main", {
                        'event_category': 'Conversions'
                    });
                });
            });
            document.querySelectorAll('.view-specific-psiholog-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    gtag("event", "view_specific_psiholog_btn_clicked_on_main", {
                        'event_category': 'Conversions'
                    });
                });
            });


            document.querySelectorAll('.ghl-frequent-questions__question-head').forEach(btn => {
                const containerEl = btn.parentElement;

                btn.addEventListener('click', (e) => {
                    if (containerEl.classList.contains('ghl-frequent-questions__question--opened')) {
                        containerEl.classList.remove('ghl-frequent-questions__question--opened')
                    } else {

                        // document.querySelectorAll('.ghl-frequent-questions__question').forEach(questionEl => {
                        //     questionEl.classList.remove('ghl-frequent-questions__question--opened');
                        // });

                        containerEl.classList.add('ghl-frequent-questions__question--opened')
                    }
                });
            });

            // document.querySelector('.problem-category-list').addEventListener('pointerdown', (e) => {
            //     console.log('pointerdown');
            // });
            //
            // document.querySelector('.problem-category-list').addEventListener('pointermove', (e) => {
            //     console.log('pointermove');
            // });
            //
            // document.querySelector('.problem-category-list').addEventListener('pointercancel', (e) => {
            //     console.log('pointercancel');
            // });

            // document.querySelector('.problem-category-list').addEventListener('pointerup', (e) => {
            //     console.log('pointerup');
            // });

            let problemDragEnabled = false;
            let previousPageX = 0;

            document.querySelector('.problem-category-list').addEventListener('touchstart', (e) => {
                console.log('touchstart');
                problemDragEnabled = true;
                const touch = e.touches[0];
                previousPageX = touch.pageX;

                console.log(document.querySelector('.problem-category-list').offsetWidth);
            });

            const baseOffset = document.querySelector('.problem-category-list').offsetLeft;

            document.querySelector('.problem-category-list').addEventListener('touchmove', (e) => {
                if (!problemDragEnabled) return;
                const touch = e.touches[0];
                const currentPageX = touch.pageX;
                const delta = previousPageX - currentPageX;
                previousPageX = touch.pageX;

                const problemCategoryListEl = document.querySelector('.problem-category-list');
                const resultingOffset = problemCategoryListEl.offsetLeft - delta;

                const itemWidth = document.querySelector('.problem-category-list__item').offsetWidth;
                const containerWidth = problemCategoryListEl.offsetWidth;
                const flexGap = 15;
                const maxOffset = containerWidth - itemWidth - flexGap * 2;

                if ((-resultingOffset) > maxOffset) {
                    problemCategoryListEl.style.left = (-maxOffset) + 'px';
                    return;
                }

                if ((resultingOffset > baseOffset)) {
                    problemCategoryListEl.style.left = (baseOffset) + 'px';
                    return;
                }

                problemCategoryListEl.style.left = (resultingOffset) + 'px';
            });

            document.querySelector('.problem-category-list').addEventListener('touchcancel', (e) => {
                problemDragEnabled = false;
            });

            document.querySelector('.problem-category-list').addEventListener('touchend', (e) => {
                problemDragEnabled = false;
            });
        });
    </script>
