        {#                    </svg>#}
        {#                    Дивитись візитку#}
        {#                </a>#}

        <div class="gh-psiholog-profile__short-price-container">
            <div class="gh-psiholog-profile__short-price-title">
                {{ 'Consultation_price'|trans }}
            </div>
            <div class="gh-psiholog-profile__short-price">
                {% if isPriceFrom %}
                    <span class="gh-psiholog-profile__short-price-from">
                            {{ 'from'|trans }}
                        </span>
                {% endif %}
                <span class="gh-psiholog-profile__short-price-value">
                            {{ minimalPrice }}
                        </span>
                <span class="gh-psiholog-profile__short-price-currency">
                            ₴
                        </span>
            </div>
        </div>
    </div>
