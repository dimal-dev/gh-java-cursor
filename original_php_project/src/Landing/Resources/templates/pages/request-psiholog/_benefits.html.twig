<div class="gh-request-psiholog__benefits">
    {% if psihologProfile is defined %}
        <div class="gh-psiholog-profile__short gh-psiholog-profile__short--static">
        <div class="gh-avatar gh-avatar--profile">
            <img class="gh-img"
                 src="{{ asset('img/select-psiholog/' ~ psihologProfile.profileTemplate ~ '.jpg', 'landing') }}"
                 alt="">
        </div>

        <h1 class="gh-psiholog-profile__short-info">
                    <span class="gh-psiholog-profile__short-name">
                        {{ psihologProfile.fullName|trans }}
                    </span>
            <span class="gh-psiholog-profile__short-trait">
                        {{ psihologProfileInfo.singlePhrase|trans|raw }}
                    </span>
        </h1>

        {#                <a href="#" class="gh-button gh-button--small gh-button--outlined-olive">#}
        {#                    <svg width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">#}
