{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-sockets": "*",
        "ext-zlib": "*",
        "amphp/http": "^1.6",
        "composer/package-versions-deprecated": "1.11.99.3",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.9",
        "domnikl/statsd": "^3.0",
        "froala/wysiwyg-editor-php-sdk": "^4.0",
        "geoip2/geoip2": "~2.0",
        "google/recaptcha": "^1.2",
        "hashids/hashids": "^4.1",
        "irazasyed/telegram-bot-sdk": "^3.6",
        "jbroadway/urlify": "^1.2",
        "league/flysystem-bundle": "^2.2",
        "php-amqplib/php-amqplib": "^2.8",
        "php-http/curl-client": "^2.1",
        "ramsey/uuid": "^3.9",
        "sendpulse/rest-api": "^1.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sentry/sdk": "^3.2",
        "sentry/sentry-symfony": "^4.2",
        "symfony/asset": "5.3.*",
        "symfony/cache": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dom-crawler": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/finder": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/lock": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/messenger": "5.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "5.3.*",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/web-link": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.14",
        "symfony/workflow": "5.3.*",
        "symfony/yaml": "5.3.*",
        "theiconic/php-ga-measurement-protocol": "^2.9",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "rector/rector": "^0.12.23",
        "symfony/css-selector": "5.0.*",
        "symfony/debug-bundle": "5.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/web-profiler-bundle": "5.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "migrate": "bin/console doctrine:migrations:migrate",
        "down": "bin/console doctrine:migrations:execute --down",
        "generate": "bin/console doctrine:migrations:generate",
        "m": "bin/console doctrine:migrations:migrate",
        "d": "bin/console doctrine:migrations:execute --down",
        "gen": "bin/console doctrine:migrations:generate"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
